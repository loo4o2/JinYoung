<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.springtest.service.BookMapper">
 	<select id="selectBookList"
 			resultType="com.test.springtest.service.Book">
 	 	SELECT
			book_id as bookId,
			book_name as bookName
		FROM test.book
 	
 	</select>
</mapper>